{
    "name": "liip/monitor-bundle",
    "type": "symfony-bundle",
    "description": "Liip Monitor Bundle",
    "keywords": ["monitoring", "monitor", "check", "health"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alvaro Videla",
            "email": "alvaro.videla@liip.ch"
        },
        {
            "name": "Kevin Bond",
            "homepage": "http://zenstruck.com/"
        },
        {
            "name": "Liip AG",
            "homepage": "http://www.liip.ch/"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://github.com/liip/LiipMonitorBundle/contributors"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "symfony/framework-bundle": "^3.4 || ^4.0 || ^5.0",
        "laminas/laminas-diagnostics": "^1.5"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^3.0 || ^4.0",
        "sensiolabs/security-checker": "^5.0 || ^6.0",
        "guzzlehttp/guzzle": "^5.3.2 || ^6.3.3",
        "symfony/expression-language": "^3.4 || ^4.0 || ^5.0",
        "swiftmailer/swiftmailer": "^5.4 || ^6.1",
        "doctrine/migrations": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "symfony/twig-bundle": "^3.4 || ^4.0 || ^5.0",
        "symfony/browser-kit": "^3.4 || ^4.0 || ^5.0",
        "symfony/asset": "^3.4 || ^4.0 || ^5.0",
        "symfony/templating": "^3.4 || ^4.0 || ^5.0",
        "phpunit/phpunit": "^7.0 || ^8.0",
        "symfony/finder": "^3.4 || ^4.0 || ^5.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "suggest": {
        "sensio/distribution-bundle": "To be able to use the composer ScriptHandler",
        "symfony/expression-language": "To use the Expression check"
    },
    "autoload": {
        "psr-4": { "Liip\\MonitorBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
