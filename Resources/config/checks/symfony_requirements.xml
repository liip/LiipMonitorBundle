<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="liip_monitor.check.symfony_requirements" public="true" class="Liip\MonitorBundle\Check\SymfonyRequirements">
            <argument>%%liip_monitor.check.symfony_requirements.file%%</argument>
            <argument>%%liip_monitor.check.symfony_requirements.label%%</argument>
            <tag name="liip_monitor.check" alias="symfony_requirements" />
        </service>
    </services>
</container>
