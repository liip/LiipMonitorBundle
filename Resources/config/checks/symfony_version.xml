<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="liip_monitor.check.symfony_version.class">Liip\MonitorBundle\Check\SymfonyVersion</parameter>
    </parameters>

    <services>
        <service id="liip_monitor.check.symfony_version" public="false" class="%liip_monitor.check.symfony_version.class%">
            <tag name="liip_monitor.check" alias="symfony_version" />
        </service>
    </services>
</container>
