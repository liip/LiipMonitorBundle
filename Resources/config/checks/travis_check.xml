<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="liip_monitor.check.travis.class">Liip\MonitorBundle\Check\TravisCheck</parameter>
    </parameters>

    <services>
        <service id="liip_monitor.check.travis" public="false" class="%liip_monitor.check.travis.class%">
            <argument>>%liip_monitor.check.travis_check.username%</argument>
            <argument>>%liip_monitor.check.travis_check.repository%</argument>
            <argument>>%liip_monitor.check.travis_check.branch%</argument>
        </service>
    </services>
</container>
